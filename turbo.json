{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"]
    },
    "clean": {
      "cache": false
    }
  },
  "globalEnv": [
    "__NEXT_PROCESSED_ENV",
    "ALGOLIA_APP_ID",
    "ALGOLIA_INDEX",
    "ALGOLIA_SEARCH_KEY",
    "ALGOLIA_WRITE_KEY",
    "ALGOLIA_QUERY_SUGGESTIONS_INDEX",
    "ALGOLIA_QUERY_SUGGESTIONS_INDEX_STORES",
    "AUTH_TOKEN",
    "AZURE_AD_CLIENT_ID",
    "AZURE_AD_CLIENT_SECRET",
    "AZURE_AD_TENANT_ID",
    "CI_ENV",
    "CLOUDINARY_API_KEY",
    "CLOUDINARY_API_SECRET",
    "DOTENV_KEY",
    "GA_MEASUREMENT_ID",
    "GA_PROTOCOL_SECRET",
    "GA_STREAM_ID",
    "GITHUB_PAT",
    "JFROG_AWS_R_TOKEN",
    "JFROG_AWS_R_REF_TOKEN",
    "JFROG_AWS_RW_TOKEN",
    "JFROG_AWS_RW_REF_TOKEN",
    "JFROG_TOKEN",
    "KV_URL",
    "KV_REST_API_URL",
    "KV_REST_API_TOKEN",
    "KV_REST_API_READ_ONLY_TOKEN",
    "MY_GITHUB_PAT",
    "NEXTAUTH_SECRET",
    "NEXT_PUBLIC_ALGOLIA_APP_ID",
    "NEXT_PUBLIC_ALGOLIA_BASE_URL",
    "NEXT_PUBLIC_ALGOLIA_ID",
    "NEXT_PUBLIC_ALGOLIA_INDEX",
    "NEXT_PUBLIC_ALGOLIA_SEARCH_KEY",
    "NEXT_PUBLIC_ALGOLIA_WRITE_KEY",
    "NEXT_PUBLIC_ALGOLIA_QUERY_SUGGESTIONS_INDEX",
    "NEXT_PUBLIC_ALGOLIA_QUERY_SUGGESTIONS_INDEX_STORES",
    "NEXT_PUBLIC_BASIC_AUTHORIZATION",
    "NEXT_PUBLIC_CLOUDINARY_API_KEY",
    "NEXT_PUBLIC_CLOUDINARY_API_SECRET",
    "NEXT_PUBLIC_GA_MEASUREMENT_ID",
    "NEXT_PUBLIC_GA_PROTOCOL_SECRET",
    "NEXT_PUBLIC_GA_STREAM_ID",
    "NEXT_PUBLIC_SANITY_API_VERSION",
    "NEXT_PUBLIC_SANITY_API_TOKEN",
    "NEXT_PUBLIC_SANITY_DATASET",
    "NEXT_PUBLIC_SANITY_PROJECT_ID",
    "NEXT_PUBLIC_VERCEL_ENV",
    "NEXT_PUBLIC_VERCEL_GIT_PROVIDER",
    "NEXT_PUBLIC_VERCEL_GIT_REPO_OWNER",
    "NEXT_PUBLIC_VERCEL_GIT_REPO_SLUG",
    "NEXT_PUBLIC_VERCEL_URL",
    "NO_COLOR",
    "NODE_ENV",
    "NPM_TOKEN",
    "OPENAI_API_KEY",
    "OPENAI_API_KEY_TWO",
    "PORT",
    "REMOTE",
    "SANITY_API_TOKEN",
    "SANITY_API_VERSION",
    "SANITY_DATASET",
    "SANITY_DATASET_TAG",
    "SANITY_PROJECT_ID",
    "SANITY_WEBHOOK_SECRET",
    "SECRET_DESTINATION_FOLDER",
    "VERCEL_ENV",
    "VERCEL_GIT_PROVIDER",
    "VERCEL_GIT_REPO_OWNER",
    "VERCEL_GIT_REPO_SLUG",
